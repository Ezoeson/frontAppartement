{
  "expo": {
    "android": {
      "permissions": ["INTERNET"]
    },
    "name": "E-appartement",
    "slug": "E-appartement",
    "icon": "./assets/images/appartement.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": ["expo-router", "expo-sqlite"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
